{
  "name": "obsidian-moon/project-utilities",
  "description": "Common tools used in Obsidian Moon projects",
  "version": "0.7.1",
  "keywords": ["library", "utilities", "traits", "interfaces"],
  "homepage": "https://github.com/obsidian-moon/project-utilities",
  "license": "MIT",
  "authors": [
    {
      "name": "Alfonso Martinez",
      "email": "alfonso@obsidian-moon.org",
      "role": "Lead Developer",
      "homepage": "https://github.com/opensaurusrex"
    }
  ],
  "support": {
    "email": "alfonso@obsidian-moon.org",
    "issues": "https://github.com/obsidian-moon/project-utilities/issues",
    "source": "https://github.com/obsidian-moon/project-utilities"
  },
  "require": {
    "php": ">=8.2",
    "sentry/sentry": "^4.3"
  },
  "require-dev": {
    "phpmd/phpmd": "^2.13",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.7",
    "wordpress/wordpress": "^6.4"
  },
  "autoload": {
    "psr-4" : {
      "ObsidianMoon\\ProjectUtilities\\" : "src/"
    }
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "wordpress/wordpress",
        "type": "webroot",
        "version": "6.4.2",
        "dist": {
          "type": "zip",
          "url": "https://wordpress.org/wordpress-6.4.2-no-content.zip"
        }
      }
    }
  ],
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
